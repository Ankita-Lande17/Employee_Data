@model List<Employee>
@using  EmployeeData.Models.Service
   <table class="table">
       <thead>
           <tr>
               <th scope="col">#</th>
               <th scope="col">Name</th>
               <th scope="col">Age</th>
               <th scope="col">Salary</th>
           </tr>
       </thead>
       <tbody>
              @foreach (var item in Model)
       {
           <tr>
                <td>@Html.ActionLink(item.EmployeeName,"Index","Employee", new { id =Common.EnryptString( item.Id.ToString() )})</td>
                <td>@item.EmployeeName</td>
                <td>@item.EmployeeAge</td>
                <td>@item.EmployeeSalary</td>
             </tr>
       }
     </tbody>
   </table>

   